# Docker Configuration
# Replace 'yourusername' with your actual Docker Hub username
DOCKER_NAMESPACE=yourusername

# Optional: Use a custom Docker registry (uncomment if needed)
# DOCKER_REGISTRY_URL=your-registry.com

# Experimental Build Configuration
# Enable experimental version support
ENABLE_EXPERIMENTAL=true

# Channel name for experimental builds
EXPERIMENTAL_CHANNEL=experimental

# Auto-sync experimental versions
AUTO_SYNC_EXPERIMENTAL=true